<div class="row mb-3">
  <div class="col-md-8 offset-md-2">
    <h1>
      Complain #<%= @complain.id %> details
    </h1>

    <div class="row mb-3">
      <div class="col">
        <a href="/complains" class="btn btn-block btn-outline-secondary">
          Go back
        </a>
      </div>

      <div class="col">
        <a href="/complains/<%= @complain.id %>/edit" class="btn btn-block btn-outline-secondary">
          Edit complain
        </a>
      </div>

      <div class="col">
        <a href="/delete_complain/<%= @complain.id %>" class="btn btn-block btn-outline-secondary">
          Delete complain
        </a>
      </div>
    </div>

    <dl>
      <dt>
        User
      </dt>
      <dd>
        <%= @complain.user_id %>
      </dd>

      <dt>
        Complaintype
      </dt>
      <dd>
        <%= @complain.complaintype_id %>
      </dd>

      <dt>
        Solved
      </dt>
      <dd>
        <%= @complain.solved %>
      </dd>

      <dt>
        Description
      </dt>
      <dd>
        <%= @complain.description %>
      </dd>

      <dt>
        Department
      </dt>
      <dd>
        <%= @complain.department_id %>
      </dd>

      <dt>
        Itemnmbr
      </dt>
      <dd>
        <%= @complain.itemnmbr %>
      </dd>

      <dt>
        Sopnumbe
      </dt>
      <dd>
        <%= @complain.sopnumbe %>
      </dd>

      <dt>
        Profile
      </dt>
      <dd>
        <%= @complain.profile_id %>
      </dd>

      <dt>
        Rating
      </dt>
      <dd>
        <%= @complain.rating %>
      </dd>

      <dt>
        Status
      </dt>
      <dd>
        <%= @complain.status %>
      </dd>

      <dt>
        Title
      </dt>
      <dd>
        <%= @complain.title %>
      </dd>

      <dt>
        Created at
      </dt>
      <dd>
        <%= time_ago_in_words(@complain.created_at) %> ago
      </dd>

      <dt>
        Updated at
      </dt>
      <dd>
        <%= time_ago_in_words(@complain.updated_at) %> ago
      </dd>
    </dl>
  </div>
</div>

