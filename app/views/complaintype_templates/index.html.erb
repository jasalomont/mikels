<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All complaintypes
    </h1>

    <a href="/complaintypes/new" class="btn btn-block btn-outline-secondary">
      Add a new complaintype
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Title
        </th>

        <th>
          Code
        </th>

        <th>
          Description
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @complaintypes.each do |complaintype| %>
      <tr>
        <!-- Display complaintype.id start -->
        <td>
          <%= complaintype.id %>
        </td>
        <!-- Display complaintype.id end -->

        <!-- Display complaintype.title start -->
        <td>
          <%= complaintype.title %>
        </td>
        <!-- Display complaintype.title end -->

        <!-- Display complaintype.code start -->
        <td>
          <%= complaintype.code %>
        </td>
        <!-- Display complaintype.code end -->

        <!-- Display complaintype.description start -->
        <td>
          <%= complaintype.description %>
        </td>
        <!-- Display complaintype.description end -->

        <!-- Display complaintype.created_at start -->
        <td>
          <%= time_ago_in_words(complaintype.created_at) %> ago
        </td>
        <!-- Display complaintype.created_at end -->

        <!-- Display complaintype.updated_at start -->
        <td>
          <%= time_ago_in_words(complaintype.updated_at) %> ago
        </td>
        <!-- Display complaintype.updated_at end -->

        <td>
          <a href="/complaintypes/<%= complaintype.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
