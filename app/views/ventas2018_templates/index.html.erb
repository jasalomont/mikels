<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All ventas2018s
    </h1>

    <a href="/ventas2018s/new" class="btn btn-block btn-outline-secondary">
      Add a new ventas2018
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12 mb-3">
    <%= render partial: "ventas2018_templates/ventas2018s_search_form" %>
  </div>
</div>



<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @ventas2018s.each do |ventas2018| %>
      <tr>
        <td>
          <%= ventas2018.id %>
        </td>

        <td>
          <%= time_ago_in_words(ventas2018.created_at) %> ago
        </td>

        <td>
          <%= time_ago_in_words(ventas2018.updated_at) %> ago
        </td>

        <td>
          <a href="/ventas2018s/<%= ventas2018.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>

    <div class="row mb-3">
      <div class="col-md-12">
        <%= paginate @ventas2018s, theme: "twitter-bootstrap-4" %>
      </div>
    </div>

  </div>
</div>
