<div class="row mb-3">
  <div class="col-md-12">
    <h1>
      All salsterrs
    </h1>

    <a href="/salsterrs/new" class="btn btn-block btn-outline-secondary">
      Add a new salsterr
    </a>
  </div>
</div>

<!-- Ransack Code start -->

<!-- Ransack Code end -->

<!-- Location Map Code start -->

<!-- Location Map Code end -->

<hr>

<div class="row mb-3">
  <div class="col-md-12">
    <table class="table">
      <tr>
        <th>
          ID
        </th>

        <th>
          Name
        </th>

        <th>
          Code
        </th>

        <th>
          Created at
        </th>

        <th>
          Updated at
        </th>

        <th>
        </th>
      </tr>

      <% @salsterrs.each do |salsterr| %>
      <tr>
        <!-- Display salsterr.id start -->
        <td>
          <%= salsterr.id %>
        </td>
        <!-- Display salsterr.id end -->

        <!-- Display salsterr.name start -->
        <td>
          <%= salsterr.name %>
        </td>
        <!-- Display salsterr.name end -->

        <!-- Display salsterr.code start -->
        <td>
          <%= salsterr.code %>
        </td>
        <!-- Display salsterr.code end -->

        <!-- Display salsterr.created_at start -->
        <td>
          <%= time_ago_in_words(salsterr.created_at) %> ago
        </td>
        <!-- Display salsterr.created_at end -->

        <!-- Display salsterr.updated_at start -->
        <td>
          <%= time_ago_in_words(salsterr.updated_at) %> ago
        </td>
        <!-- Display salsterr.updated_at end -->

        <td>
          <a href="/salsterrs/<%= salsterr.id %>">
            Show details
          </a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
